{
  "name": "@vikadata/widget-cli",
  "description": "help you build awesome vika widget",
  "version": "0.1.0",
  "author": "vikadata",
  "bin": {
    "widget-cli": "./bin/run"
  },
  "bugs": "https://github.com/vikadata/widget-cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@types/archiver": "^5.1.1",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.12",
    "@types/gitignore-parser": "^0.0.0",
    "@types/glob": "^7.1.4",
    "@types/semver": "^7.3.7",
    "@vikadata/vika": "^1.0.3",
    "archiver": "^5.3.0",
    "archiver-zip-encrypted": "^1.0.10",
    "axios": "^0.21.1",
    "chalk": "^4.1.1",
    "express": "^4.17.1",
    "find-up": "^5.0.0",
    "form-data": "^4.0.0",
    "fs-extra": "^10.0.0",
    "gitignore-parser": "^0.0.2",
    "glob": "^7.1.7",
    "semver": "^7.3.5",
    "ts-loader": "^9.2.3",
    "tslib": "^1",
    "webpack": "^5.45.1",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "chai": "^4",
    "eslint": "^7.11.0",
    "eslint-config-vika": "^1.0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/vikadata/widget-cli",
  "keywords": [
    "vika",
    "widget",
    "widget-cli"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "widget-cli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "vikadata/widget-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
